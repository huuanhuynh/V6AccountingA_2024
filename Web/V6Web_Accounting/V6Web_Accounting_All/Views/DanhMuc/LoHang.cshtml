
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Lô Hàng</title>
    <link href="~/Content/css/V6.css" rel="stylesheet" />
    <link href="~/Content/css/V6WebControls.css" rel="stylesheet" />

    <link href="~/Content/css/themes/base/jquery.ui.datepicker.css" rel="stylesheet" />

    <script src="~/Scripts/core/jquery/jquery-2.1.0.min.js"></script>
    
    <script src="~/Scripts/lib/jquery-ui/jquery-ui-1.10.4.min.js"></script>
    <script src="~/Scripts/app/controls/V6Tab.js"></script>

    <link rel="shortcut icon" href="~/V6Soft.ico" />

    <style type="text/css">
        #ui-datepicker-div {
            display: none;
        }
    </style>
</head>
<body>
    <div> 
        <h1 style="text-align:center">Thông tin lô hàng</h1>

        <div id="Panel1" class="biggroup normalsize borderconer graybackground" style="height:80px;margin-top:4px;">
            <div id="ad11" style="float:left;width:350px;">
                <span>Mã vật tư</span>
                <input type="text" id="ma_vt" /><br />
                <span>Mã khách hàng</span>
                <input type="text" id="ma_kh" /><br />
                <span>Số lô</span>
                <input type="text" id="so_lo" /><br />
            </div>
            <div id="ad11" style="float:left;width:350px;">
                <span>Số đk</span>
                <input type="text" id="so_dang_ky" /><br />
                <span>Tên lô</span>
                <input type="text" id="ten_lo" /><br />
                <span>Tên 2</span>
                <input type="text" id="ten_lo2" /><br />
            </div>
        </div>

        <div id="Tab" class="tabsbutton">
            <span id="Span1" tabid="0" class="active-tab-btn">Thông tin chính</span>
            <span id="Span2" tabid="1">Thông tin tự định nghĩa</span>
            <span id="Span3" tabid="2" title="Hướng dẫn sử dụng trang. Nếu bạn còn lúng túng trong cách sử dụng thì bạn nên xem qua trang hướng đẫn này!">Hướng dẫn</span>
        </div>
        <div id="Tab_content" class="tab-content graybackground" style="height:auto;">

            <div class="tab-pane show" id="Tab_0">
                <div id="tabpage01" class="normalsize" style="height:150px;">
                    <div id="am11" style="width:350px;float:left;">
                        <span>Ngày nhập</span>      <input type="text" id="thu_kho"     class="v6Date" /> <br />
                        <span>Ngày sản xuất</span>  <input type="text" id="dien_thoai"  class="v6Date" /> <br />
                        <span>Ngày bắt đầu sd</span><input type="text" id="email"       class="v6Date" /> <br />
                        <span>Ngày kiểm tra</span>  <input type="text" id="fax"         class="v6Date" /> <br />
                        <span>Ngày hết hạn sd</span><input type="text" id="iskhodaily"  class="v6Date" /> <br />
                        <span>Ngày bảo hành</span>  <input type="text" id="tkdl"        class="v6Date" /> <br />
                    </div>

                    <div id="tab1right" style="width:350px;float:left;">
                        <span>Mã vt 2</span>  <input type="text" id="ma_vt2" />   <br />
                        <span>Barcode</span>   <input type="text" id="bar_code" class="v6Uppercase"/>    <br />
                        <span>Trạng thái</span><input type="checkbox" id="status" class="checkbox" /><br />
                        
                        <span>Ghi chú</span> <textarea></textarea><br />
                    </div>

                </div>
            </div>

            <div class="tab-pane" id="Tab_1">
                <div id="AMshowhide" class="normalsize" style="height:150px;">
                    <div id="am11" style="width:350px;float:left;">
                        <span>Mã 1</span>   <input type="text" id="ma_td1" class="v6Uppercase" value="" /><br />
                        <span>Mã 2</span>   <input type="text" id="ma_td2" class="v6Uppercase" value="" /><br />
                        <span>Mã 3</span>   <input type="text" id="ma_td3" class="v6Uppercase" value="" /><br />

                        <span>Ngày 1</span>   <input type="text" id="ngay_td1" class="v6Date" value="" /><br />
                        <span>Ngày 2</span>   <input type="text" id="ngay_td2" class="v6Date" value="" /><br />
                        <span>Ngày 3</span>   <input type="text" id="ngay_td3" class="v6Date" value="" /><br />
                    </div>
                    <div id="am11" style="width:350px;float:left;">
                        <span>Số lượng 1</span>   <input type="text" id="sl_td1" class="v6Uppercase v6Number" value="" /><br />
                        <span>Số lượng 2</span>   <input type="text" id="sl_td2" class="v6Uppercase v6Number" value="" /><br />
                        <span>Số lượng 3</span>   <input type="text" id="sl_td3" class="v6Uppercase v6Number" value="" /><br />

                        <span>Ghi chú 1</span>   <input type="text" id="gc_td1" class="v6Uppercase" value="" /><br />
                        <span>Ghi chú 2</span>   <input type="text" id="gc_td2" class="v6Uppercase" value="" /><br />
                        <span>Ghi chú 3</span>   <input type="text" id="gc_td3" class="v6Uppercase" value="" /><br />
                    </div>

                </div>
            </div>

            <div id="Tab_2" class="tab-pane" tabid="2">
                <div id="DivHD1" class="pagehelp"
                     style="width:980px;height:auto;">
                    <h3 style="text-align:center">Chưa có hướng dẫn</h3>
                    <div id="help_content" style="text-align:justify">
                        <h4>Chưa có dữ liệu.</h4>
                        <p>
                            Nodata!
                        </p>
                    </div>
                </div>
            </div>
        </div>
    
        
    </div>
    
    <script src="~/Scripts/lib/jquery.mousewheel.js"></script>

    <script src="~/Scripts/app/controls/V6ControlsEvent.js"></script>
    <script src="~/Scripts/app/controls/V6Popup.js"></script>
    
    <script src="~/Scripts/app/controls/V6Number.js"></script>
    <script src="~/Scripts/app/controls/V6WebControls.js"></script>

    
</body>
</html>
