  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="NetTcpBinding_ICustomerService">
          <security mode="None" />
        </binding>
        <binding name="NetTcpBinding_IAssistantService">
          <security mode="None" />
        </binding>
        <binding name="NetTcpBinding_IDefinitionService">
          <security mode="None" />
        </binding>
        <binding name="NetTcpBinding_IMenuService">
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint binding="netTcpBinding" bindingConfiguration="NetTcpBinding_ICustomerService" contract="CustomerService.ICustomerService" name="NetTcpBinding_ICustomerService" behaviorConfiguration="Service_NoChainTrust" />
      <endpoint binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IAssistantService" contract="AssistantService.IAssistantService" name="NetTcpBinding_IAssistantService" behaviorConfiguration="Service_NoChainTrust" />
      <endpoint binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IDefinitionService" contract="DefinitionService.IDefinitionService" name="NetTcpBinding_IDefinitionService" behaviorConfiguration="Service_NoChainTrust" />
      <endpoint binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IMenuService" contract="MenuService.IMenuService" name="NetTcpBinding_IMenuService" behaviorConfiguration="Service_NoChainTrust" />
    </client>
    <behaviors>
      <endpointBehaviors>
        <behavior name="Service_NoChainTrust">
          <clientCredentials>
            <serviceCertificate>
              <authentication certificateValidationMode="ChainTrust" revocationMode="NoCheck" />
            </serviceCertificate>
          </clientCredentials>
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>
